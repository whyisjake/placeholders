{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "placeholders/small-square",
	"title": "Small Square (200×200)",
	"category": "placeholders",
	"icon": "format-image",
	"description": "Small Square ad placeholder - 200×200 pixels",
	"keywords": ["ad", "placeholder", "small", "square", "200x200"],
	"textdomain": "placeholders",
	"supports": {
		"html": false,
		"align": ["wide", "full"],
		"color": {
			"background": true,
			"text": true
		}
	},
	"attributes": {
		"backgroundColor": {
			"type": "string",
			"default": "#f0f0f0"
		},
		"textColor": {
			"type": "string",
			"default": "#666666"
		}
	}
}
